alias ..1="cd .."
alias ..="cd .."
alias cd..="cd .."
alias cd1="cd .."

alias ..2="cd ../.."
alias ...="cd ../.."
alias cd...="cd ../.."
alias cd2="cd ../.."

alias ..3="cd ../../.."
alias ....="cd ../../.."
alias cd....="cd ../../.."
alias cd4="cd ../../.."

alias ..4="cd ../../../.."
alias .....="cd ../../../.."
alias cd.....="cd ../../../.."
alias cd4="cd ../../../.."

alias ll="ls -lh"
alias la="ls -a"
alias lla="ls -lha"
alias lf="ls -F"
alias ld="ls -d */"
alias ltr="ls -tr"
alias lzip="ls *.zip"
alias lrar="ls *.rar"

alias pu="pushd"
alias po="popd"
alias dirs="dirs -v"

# for networking restart
alias restartnetworking="sudo /etc/init.d/networking restart"

# alias for apt
alias acs="sudo apt-cache seach"
alias agu="sudo apt-get update"
alias agi="sudo apt-get install"
alias agg="sudo apt-get upgrade"
alias agd="sudo apt-get dist-upgrade"
alias agr="sudo apt-get remove"

# for screen
alias scr="screen -r"
alias scls="screen -ls"

# for nautilus
alias nautilus="nautilus --no-desktop"

# for oceantarget
export ot="/windows/sda5/oceantarget"
alias cdot="cd $ot"

# for du
alias duh="du . -h | grep -v /"


alias vum="vim -c 'source ~/.vumrc'"
alias grep="grep --exclude=*svn* --color -n"

