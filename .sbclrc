
;;; The following lines added by ql:add-to-init-file:
#-quicklisp

(defun init-local-packages-path ()
  (eval (read-from-string "(push #P\"/data/gaiatest/\" asdf:*central-registry*)")))

(defun load-local-packages ()
  (handler-case
    (eval (read-from-string "(ql:quickload :tpbackup)"))
    (condition () "tpbackup load error!")))

(let ((quicklisp-init #P"/data/quicklisp/setup.lisp"))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)
    (init-local-packages-path)
    (load-local-packages)))


